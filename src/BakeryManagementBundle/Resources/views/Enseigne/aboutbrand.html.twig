    <script>
        function rating() {

            var note = 0;
            star1 = $('#star1');
            star2 = $('#star2');
            star3 = $('#star3');
            star4 = $('#star4');
            star5 = $('#star5');

            if (star1.is(':checked')) {
                note = 5;
            }
            else if (star2.is(':checked')) {
                note = 4;
            }
            else if (star3.is(':checked')) {
                note = 3;
            }
            else if (star4.is(':checked')) {
                note = 2;
            }
            else if (star5.is(':checked')) {
                note = 1;
            }
            else {
                note = 0;
            }


//              alert($('#niveau').val());
            var url = '{{ path('rate') }}';
            $.ajax({
                url: url,
                type: 'POST',
                data: 'n=' + note + '&d=' +{{ enseignes.id }},
                success: function (data) {
                    if (data !== null) {
                        //alert("aaa");
                        console.log(data);

                    }
                    else {

                    }
                },
                error: function () {

                }
            });

        }


    </script>
